      <!-- forms -->
      <div itemscope itemtype="http://schema.org/LocalBusiness" itemref="_name1 _address3" id="forms">
        <!-- submit business form -->
        <h4>Know a dog-friendly business in Omaha? Tell us! Once we confirm it, we'll post it here on the site. </h4>

        <div id="businessform" class="center">
          <div id="thanks"></div>
          <form action="/contacts" method="post"> 
            <input type="text" class="form" placeholder="business name" name="name"required></h2>  
            <input type="hidden" name="date" value="<%=Time.now%>"> <input type="submit" value="submit" id="submit" class="submit "> 
          </form>
        </div>

        <!-- get stick form -->
        <h4>Put an Omaha.dog sticker on your door so customers know you're dog-friendly! 
          <br>Request one here, and we'll mail it to you.</h4>

          <div id="stickerform" class="center">
            <div id="thanks-two"></div>
            <form action="/stickers" method="post"> 
              <input type="text" class="form" placeholder="business name" name="name" required></h2> 
              <input type="text" class="form" placeholder="business address" name="address" required></h2>  
              <input type="hidden" name="date" value="<%=Time.now%>"> <input type="submit" value="submit" id="submit" class="submit"> 
            </form>
          </div>
